{
  "name": "setup-env",
  "version": "0.1.0",
  "description": "This is an example",
  "main": "index.js",
  "scripts": {
    "clean": "node ./scripts/clean-dist.js",
    "build:webpack": "webpack --config ./webpack/webpack.config.prod.js",
